{
	"name": "Obscure",
	"version": "0.1",
	"manifest_version": 2,
 	"permissions": [
          "history","tabs","activeTab","storage"
    ],
	"description": "Effortlessly be discreet",
	"background":{
		"scripts":["background.js"]
	},
	"browser_action": {	
		"default_icon": "Obscurelogo.png",
		"default_title": "The easiest cloak of obscurity!",
		"default_popup": "extension.html"
	},
	"content_security_policy": "script-src 'self' https://www.google.com; object-src 'self'",
	"icons": {
		"128": "Obscurelogo.png"
	}
}